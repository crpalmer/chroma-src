  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.3.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/jonschlinkert/toggle-array",
  "license": "MIT",
  "main": "index.js",
  "name": "toggle-array",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/toggle-array.git"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "prompt-choices",
        "arr-flatten",
        "array-unique"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "version": "1.0.1"
}