{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "dependencies": {
    "debug": "^2.6.8",
    "prompt-checkbox": "^2.2.0"
  },
  "description": "Radio prompt. Can be used as a standalone prompt, or as a plugin for [Enquirer].",
  "devDependencies": {
    "ansi-colors": "^0.2.0",
    "enquirer": "^1.0.2",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.4.2",
    "strip-color": "^0.1.0"
  },
  "engines": {
    "node": ">=5.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/enquirer/prompt-radio",
  "license": "MIT",
  "main": "index.js",
  "name": "prompt-radio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enquirer/prompt-radio.git"
  },
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "data": {
      "alias": "radio"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "prompt-base",
        "prompt-checkbox",
        "prompt-confirm",
        "prompt-question"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "version": "1.2.1"
}