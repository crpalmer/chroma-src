{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "dependencies": {
    "debug": "^2.6.6",
    "extend-shallow": "^2.0.1",
    "log-utils": "^0.2.1"
  },
  "description": "Paginator used for rendering lists or choices for prompts in the terminal.",
  "devDependencies": {
    "gulp-format-md": "^0.1.12"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/enquirer/terminal-paginator",
  "license": "MIT",
  "main": "index.js",
  "name": "terminal-paginator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enquirer/terminal-paginator.git"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "choices-separator",
        "enquirer",
        "prompt-choices",
        "readline-utils"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "version": "2.0.2"
}